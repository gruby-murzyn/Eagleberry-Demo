
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{
Layout = null;
}
@* the fun starts here *@
<!DOCTYPE html>
<html lang="en">
   <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <meta name="description" content="">
      <meta name="author" content="">
      <title>@Umbraco.Field("pageTitle", casing: RenderFieldCaseType.Upper)</title>
      <!-- Bootstrap core CSS -->
      <!-- Bootstrap -->
      <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
      <!--custom css -->
     
      <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="/css/MinStyle.css ">
      <link rel="stylesheet" type="text/css" href="css/bootstrap-lightbox.css ">
      <link rel="stylesheet" type="text/css" href="/css/bootstrap-lightbox.min.css ">
    
     
      
      
      
      

   </head>




         <!-- Navigation -->
@Umbraco.RenderMacro("SiteNavigation")
  
<!-- content-->

   <div class="container">

       <div class="row">
        <div class="col-sm-8">
            <div class="topbannerMedia">
                                  <iframe width="740" height="416" src="https://www.youtube.com/embed/nTDNLUzjkpg" frameborder="0" gesture="media" allowfullscreen></iframe>
                              </div
            <article>
                @Html.GetGridHtml(Model.Content, "bodyText", "bootstrap3-fluid")
            </article>
            </div>
            
            <div class="col-sm-4 sidebar-right">
    
            <div class="post">
                @Html.Partial("categories")
                
            </div>
        </div>
        </div>





   @RenderBody()
   
   <!-- Footer -->

  <!-- Footer -->
